{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "https://eventora2.onrender.com/api",
        "API_BASE_URL": "https://eventora2.onrender.com",
        "EXPO_PUBLIC_API_URL": "https://eventora2.onrender.com/api",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_URL": "https://eventora2.onrender.com/api",
        "API_BASE_URL": "https://eventora2.onrender.com",
        "EXPO_PUBLIC_API_URL": "https://eventora2.onrender.com/api",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "API_URL": "https://eventora2.onrender.com/api",
        "API_BASE_URL": "https://eventora2.onrender.com",
        "EXPO_PUBLIC_API_URL": "https://eventora2.onrender.com/api",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}